{
    "name": "sort",
    "primitive_name": "sort",
    "aliases": [
        "sort_array",
        "array_sort"
    ],
    "type": "PRIMITIVE_OPERATION",
    "description": [
        "Sort `input` wrt `dimension_index` in ascending order by value"
    ],
    "inputs": [
        "input",
        "dimension_index"
    ],
    "outputs": [
        "sorted_array"
    ],
    "assertions": [
        "{dimension_index}_is_scalar",
        "{dimension_index}_is_greater_than_or_equal_to_zero",
        "{dimension_index}_is_less_than_{input}_number_of_dimensions",
        "{dimension_index}_data_type_is_integer",
        "{input}_is_array",
        "{sorted_array}_is_array",
        "{sorted_array}_data_type_is_the_same_as_{input}",
        "{sorted_array}_shape_is_the_same_as_{input}"
    ],
    "examples": [
        {
            "inputs": [
                {
                    "name": "input",
                    "data": [
                        [
                            5,
                            4,
                            3
                        ],
                        [
                            2,
                            1,
                            0
                        ]
                    ],
                    "type": "INTEGER"
                },
                {
                    "name": "dimension_index",
                    "data": 1,
                    "type": "INTEGER"
                }
            ],
            "outputs": [
                {
                    "name": "sorted_array",
                    "data": [
                        [
                            3,
                            4,
                            5
                        ],
                        [
                            0,
                            1,
                            2
                        ]
                    ]
                }
            ]
        },
        {
            "inputs": [
                {
                    "name": "input",
                    "data": [
                        [
                            5,
                            4,
                            3
                        ],
                        [
                            2,
                            1,
                            0
                        ]
                    ],
                    "type": "INTEGER"
                },
                {
                    "name": "dimension_index",
                    "data": 0,
                    "type": "INTEGER"
                }
            ],
            "outputs": [
                {
                    "name": "sorted_array",
                    "data": [
                        [
                            2,
                            1,
                            0
                        ],
                        [
                            5,
                            4,
                            3
                        ]
                    ]
                }
            ]
        }
    ]
}
